{
  "name": "giftraptor",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "watch": "yarn watch:server & yarn watch:js & yarn watch:css",
    "watch:server": "nodemon --config nodemon.server.json --exec node server.js",
    "watch:js": "watchify src/js/entries/app.js -v -t babelify -g envify -p livereactload -o public/app.js",
    "watch:css": "nodemon --exec node-sass src/css/app.sass public/app.css --config nodemon.sass.json",
    "migrate": "db-migrate up && yarn dump",
    "dump": "pg_dump -d giftraptor_development --schema-only -O -f schema.sql && pg_dump -d giftraptor_development -a -t migrations -f schema_migrations.sql",
    "prod:bundle": "NODE_ENV=production browserify src/js/bundles/app_bundle.js -t babelify -g envify -g uglifyify > public/app.js",
    "prod:sass": "NODE_ENV=production node-sass src/css/app.sass public/app.css --output-style=compressed"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "babel-preset-latest": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-2": "^6.22.0",
    "babelify": "^7.3.0",
    "bcrypt": "^1.0.2",
    "bluebird": "^3.4.7",
    "body-parser": "^1.16.1",
    "bootstrap-sass": "^3.3.7",
    "classnames": "^2.2.5",
    "connect-flash": "^0.1.1",
    "connect-redis": "^3.2.0",
    "db-migrate": "^0.10.0-beta.20",
    "db-migrate-pg": "^0.1.11",
    "dotenv": "^4.0.0",
    "envify": "^4.0.0",
    "express": "^4.14.1",
    "express-session": "^1.15.1",
    "immutable": "^3.8.1",
    "livereactload": "^3.2.0",
    "lodash": "^4.17.4",
    "method-override": "^2.3.7",
    "node-sass": "^4.5.0",
    "passport": "^0.3.2",
    "passport-facebook": "^2.1.1",
    "passport-local": "^1.0.0",
    "pg": "^6.1.2",
    "pm2": "^2.4.3",
    "pug": "^2.0.0-beta11",
    "react": "^15.4.2",
    "react-bootstrap": "^0.30.7",
    "react-bootstrap-typeahead": "^1.2.0",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.2",
    "react-router": "^3.0.2",
    "redux": "^3.6.0",
    "redux-form": "^6.5.0",
    "reselect": "^2.5.4",
    "slugg": "^1.1.0",
    "squel": "^5.7.0",
    "superagent": "^3.4.4",
    "uglifyify": "^3.0.4",
    "yarn": "^0.20.3"
  },
  "devDependencies": {
    "babel-plugin-react-transform": "^2.0.2",
    "eslint": "^3.15.0",
    "eslint-plugin-react": "^6.10.0",
    "nodemon": "^1.11.0",
    "react-proxy": "^1.1.8",
    "watchify": "^3.9.0"
  }
}
